.navbar.navbar-default.navbar-static-top.navbar-fixed-top
  .container
    %button.navbar-toggle{ type: 'button', data: { toggle: 'collapse', target: '.navbar-responsive-collapse', class: "container" }}
      %span.icon-bar
      %span.icon-bar
      %span.icon-bar
    = link_to '/', class: 'navbar-brand' do
      = image_tag("logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "36px", class: "navbar-brand")
    .navbar-collapse.collapse.navbar-responsive-collapse
      //not login
      %ul.nav.navbar-nav.navbar-right
        %li
          .btn.btn-primary.navbar-btn
            = link_to 'Get Started', ''

      //login
      %ul.nav.navbar-nav.navbar-right
        %li.dropdown
          %a.dropdown-toggle{ area: { expanded: false }, data: { toggle: 'dropdown' }, href: '#', role: 'button', class: 'dropdown' }
            UserName
            %span.caret
          %ul.dropdown-menu{ role: 'menu' }
            %li
              = link_to 'Profile Edit', '#'
            %li
              = link_to'Logout', '#'
        %li
          .btn.btn-primary.navbar-btn
            = link_to 'New Proto', '/prototypes/new'
